{
  "name": "vendi-advertising/vendi-yaml-loader",
  "license": "MIT",
  "description": "Generic YAML loader used by several Vendi libraries",
  "authors": [
    {
      "name": "Chris Haas",
      "email": "chris@vendiadvertising.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Vendi\\YamlLoader\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Vendi\\YamlLoader\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.0|^8.1|^8.2",
    "symfony/yaml": "^5.0.8|^6",
    "symfony/filesystem": "^6.2"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "mikey179/vfsstream": "^1.6",
    "phpunit/phpunit": "^8|^9"
  }
}
